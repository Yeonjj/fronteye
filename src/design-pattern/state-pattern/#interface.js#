        this.hasPin = new HasPin(this);
        this.noCash = new NoCash(this);
        this.atmState = noCard;
        this.cashInMachine = 2000;
        this.correctPinEntered = 0;
    }

    insertCard(){
        atmState.insertCard();
    }

    ejectCard(){
        atmState.ejectCard();
    }

    insertPin(pinNumber){
        atmState.insertPin(pinNumber);
    }

    requesteCash(money){
        atmState.requesteCash(money);
    }
}

